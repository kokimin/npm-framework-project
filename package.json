{
  "name": "npm-framework-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:github": "VITE_BASE_PATH=/npm-framework-project/ VITE_OUT_DIR=github-dist vite build && touch github-dist/.nojekyll",
    "preview": "vite preview",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/"
  },
  "dependencies": {
    "@kokimin/utils": "^0.0.11",
    "@kokimin/vue-framework": "^0.0.22",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@rushstack/eslint-patch": "^1.15.0",
    "@trivago/prettier-plugin-sort-imports": "^6.0.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "~1.35.0",
    "eslint-plugin-vue": "~10.6.2",
    "globals": "^16.5.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.35.0",
    "prettier": "3.7.4",
    "vite": "^7.3.0",
    "vite-plugin-vue-devtools": "^8.0.5"
  }
}
